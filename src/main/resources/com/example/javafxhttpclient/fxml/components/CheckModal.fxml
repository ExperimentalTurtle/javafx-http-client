<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="150.0"
            prefWidth="600.0"
            style="-fx-background-color: fx-primary"
            xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.javafxhttpclient.core.fxml.CheckModalController">

    <Text fx:id="checkText"
          AnchorPane.topAnchor="20"
          AnchorPane.leftAnchor="20"
          AnchorPane.rightAnchor="0"
          strokeType="OUTSIDE"
          strokeWidth="0.0"
          text="Are you sure you want to do this ?"/>

    <Button fx:id="noButton"
            layoutX="495.0"
            AnchorPane.bottomAnchor="8"
            mnemonicParsing="false"
            prefHeight="24.0"
            prefWidth="91.0"
            text="No"/>

    <Button fx:id="yesButton"
            layoutX="377.0"
            AnchorPane.bottomAnchor="8"
            mnemonicParsing="false"
            prefHeight="24.0"
            prefWidth="106.0"
            text="Yes"/>
</AnchorPane>
